<mujoco model="tracer">
    <compiler angle="radian" meshdir="meshes/" />
    <asset>
        <mesh name="base_link_aloha" file="meshes/tracer_mobile_base/base_link_aloha.STL" />
    </asset>

    <worldbody>
        <body name="mobile_base" pos="0 0 0">
            <inertial pos="0 0 0" mass="0.004" diaginertia="0.0001 0.0006 0.0005" />
            <body name="support" pos="0 -0.2 0">
                <!-- add robot here -->
                <site name="center" type="sphere" pos="0 0 0" size="0.01" group="1" rgba="0 0 0 0"/>
            </body>
            <body name="wheeled_base" pos="0 -0.2 0">
                <inertial pos="0 0 0" mass="0.004" diaginertia="0.0001 0.0006 0.0005" />
                <geom pos="0 0 0" euler="0 0 0" type="mesh" group="1" density="7.85" mass="10000"
                rgba="0.79216 0.81961 0.93333 1" mesh="base_link_aloha" />
            </body>
            <joint name="joint_mobile_side" pos="0 0 0" axis="1 0 0" type="slide" limited="false" damping="0" armature="0.0" frictionloss="200"/>
            <joint name="joint_mobile_forward" pos="0 0 0" axis="0 1 0" type="slide" limited="false" damping="0" armature="0.0" frictionloss="100"/>
            <joint name="joint_mobile_yaw" pos="0 0 0" axis="0 0 1" type="hinge" limited="false" damping="0" armature="0.0" frictionloss="200"/>
        </body>
    </worldbody>
    <actuator>
        <velocity ctrllimited="true" ctrlrange="-1.00 1.00" joint="joint_mobile_forward" kv="2000" name="actuator_mobile_forward" forcelimited="true" forcerange="-16000 16000"/>
		<velocity ctrllimited="true" ctrlrange="-1.00 1.00" joint="joint_mobile_side" kv="2000" name="actuator_mobile_side" forcelimited="true" forcerange="-2200 2200"/>
		<velocity ctrllimited="true" ctrlrange="-1.50 1.50" joint="joint_mobile_yaw" kv="1500" name="actuator_mobile_yaw" forcelimited="true" forcerange="-600 600"/>
	</actuator>
</mujoco>